import random
waypointsN = []
waypointsX = []
waypointsY = []
xS = 0
yS = 0
data = []
biome = [xS, yS, data]
Biomes = []
x = 0
y = 0
command = 0
wps = 0
name = 0
class chunk():
	def _init_(self):
		self.x = ""
		self.y = ""
		self.biome = ""

def NewTerrain():
	water = False
	TerrainWater = ["creek","river","lake","stream","waterfall"]
	TerrainGround = ["grassy","rocky","sandy"]
	TerrainAttribute = ["trees","cliffs"]
	TerrainLevel = ["mountains","lowlands","highlands"]
	
	water = random.choice([True, False])
	TGF = random.choice(TerrainGround)
	TAF = random.choice(TerrainAttribute)
	TLF = random.choice(TerrainLevel)
	if water == True:
		TWF = random.choice(TerrainWater)
		print ("There are", TGF, TLF, "with", TAF, "and a", TWF)
		# xS = x #copy.copy(x)
		# yS = y
		# TGFS = TGF
		# TLFS = TLF
		# TAFS = TLF
		# TWFS = TWF
		data.extend([[x,y],[TGF,TLF,TAF,TWF]])
		# Biomes.append(biome)
		# print(Biomes)
	else:
		print ("There are", TGF, TLF, "with", TAF)
		# xS = x
		# yS = y
		# TGFS = TGF
		# TLFS = TLF
		# TAFS = TLF 
		data.extend([[x,y],[TGF,TLF,TAF]])
		# Biomes.extend(biome)
		# print(Biomes)


loop = True
while loop:
	command = input()
	if command == "help":
		print ("forward")
		print ("back")
		print ("left")
		print ("right")
		print ("waypoint")
		print ("help")
	elif command == "forward":
		x = x + 1
		print ("your coordinates are: x = ",x,", y = ",y)
		NewTerrain()
	elif command == "back":
		x = x - 1
		print ("your coordinates are: x = ",x,", y = ",y)
		NewTerrain()
	elif command == "left":
		y = y + 1
		print ("your coordinates are: x = ",x,", y = ",y)
		NewTerrain()
	elif command == "right":
		y = y - 1
		print ("your coordinates are: x = ",x,", y = ",y)
		NewTerrain()
	elif command == "waypoint":
		print("action:")
		action = input()
		if action == "add":
			if wps < 5:
				print("name:")
				action = input()
				waypointsN.append(action)
				waypointsX.append(x)
				waypointsY.append(y)
				wps = wps + 1
				print("waypoint",action,"has been added")
			elif wps >= 5:
				print("Too many waypoints")
		elif action == "list":
			print("name: ",waypointsN)
			print("x: ",waypointsX)
			print("y: ",waypointsY)
		elif action == "delete":
			print("waypoint name:")
			name = input()
			if waypointsN[0] == name:
				del waypointsN[0]
				del waypointsX[0]
				del waypointsY[0]
				print("waypoint",name,"has been deleted")
			elif waypointsN[1] == name:
				del waypointsN[1]
				del waypointsX[1]
				del waypointsY[1]
				print("waypoint",name,"has been deleted")
			elif waypointsN[2] == name:
				del waypointsN[2]
				del waypointsX[2]
				del waypointsY[2]
				print("waypoint",name,"has been deleted")
			elif waypointsN[3] == name:
				del waypointsN[3]
				del waypointsX[3]
				del waypointsY[3]
				print("waypoint",name,"has been deleted")
			elif waypointsN[4] == name:
				del waypointsN[4]
				del waypointsX[4]
				del waypointsY[4]
				print("waypoint",name,"has been deleted")
			else:
				print("no waypoint",name,"found")
		elif action == "help":
			print("help")
			print("add")
			print("delete")
			print("list")
